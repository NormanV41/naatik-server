{
  "name": "servicio-sap-app-server",
  "version": "1.0.0",
  "description": "server for servicio-sap-app",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.ts\" \"**/*.html\" \"**/*.scss\" \"**/*.json\"",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc -p . && node ./lib/index.js",
    "inspect": "tsc -p . && node --inspect-brk lib/index.js && rm -r lib/",
    "lint": "tslint \"src/**/*.ts\""
  },
  "author": "\"Norman Vasquez <nvasquez@serviciosap.com>",
  "license": "UNLICENSED",
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.5.35",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.22",
    "nodemon": "^1.19.4",
    "prettier": "1.18.2",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "@types/nodemailer": "^6.4.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.7",
    "colorts": "^0.1.63",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "mongodb": "^3.4.1",
    "mongoose": "^5.8.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nocache": "^2.1.0",
    "nodemailer": "^6.4.2"
  }
